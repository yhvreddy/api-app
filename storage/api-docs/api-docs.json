{
    "openapi": "3.0.0",
    "info": {
        "title": "Swagger with Laravel APIs",
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/user": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get all Users List",
                "operationId": "babaf8a5ff0bc9f2e918c6fe96b2ade8",
                "responses": {
                    "default": {
                        "description": "successful operation"
                    }
                }
            },
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Creating new user",
                "operationId": "b06edc54e183ec3754acab6da6f77d26",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "description": "User name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "User's email",
                        "required": true,
                        "schema": {
                            "type": "email"
                        }
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "User's password",
                        "required": true,
                        "schema": {
                            "type": "password"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "User created successful"
                    },
                    "401": {
                        "description": "Failed to create user"
                    }
                }
            }
        },
        "/api/v1/user/{user}": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get logged-in user details",
                "operationId": "eaf690f9636aec5184475b8f76859e82",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "User id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Users"
                ],
                "summary": "Hard delete user details",
                "operationId": "db5180547ea70143a55d55a175f97799",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "User id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/api/v1/user/{user}/trashed": {
            "delete": {
                "tags": [
                    "Users"
                ],
                "summary": "Soft delete user details",
                "operationId": "21001f13d46ebc91b23b6d4b85cf1fec",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "User id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/api/v1/user/{user}/restore": {
            "patch": {
                "tags": [
                    "Users"
                ],
                "summary": "Retore soft deleted user details",
                "operationId": "84fa4b3b1d8bafa3c96f1cb958b0625c",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "User id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Authenticate user and generate token",
                "operationId": "88066443cc1a490fa39e40b6cafd5ff9",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "User's email",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "User's password",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Login successful"
                    },
                    "401": {
                        "description": "Invalid credentials"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "sanctum",
                "scheme": "bearer"
            }
        }
    }
}